    "lodash": "^4.17.21",
    "micromark": "^3.1.0",
    "micromark-extension-gfm": "^2.0.1",
    "monaco-editor-vue3": "^0.1.6",
    "monaco-editor-webpack-plugin": "^7.0.1",
    "node-fetch": "^3.2.4",
    "sanitize-html": "^2.8.1",
    "tawk-messenger-vue-3-ts": "^1.0.1",
    "vue": "^3.0.0",
    "vue-cookies": "^1.8.1",
    "vue-gtag": "^2.0.1",
    "vue-i18n": "^9.1.0",
    "vue-resize-observer": "^2.0.15",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.2"
  },
